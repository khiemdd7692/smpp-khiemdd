{
    "name": "khiemdd/smpp",
    "description": "php smpp",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "php",
        "smpp",
        "swoole",
        "hyperf"
    ],
    "autoload": {
        "psr-4": {
            "SMPP3\\": "src/"
        }
    },
    "authors": [
        {
            "name": "khiemdd",
            "email": "khiemdd"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "ext-swoole": ">=4.4"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "SMPP3\\ConfigProvider"
        }
    }
}
